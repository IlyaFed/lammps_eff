LAMMPS (11 Aug 2017)
#hydrogen

variable        my_temp index 2000 5000 8000 12000 15000 20000 25000 30000 35000 40000 45000
variable        old_step equal 0
variable        Step equal step
variable        dumpstep equal 1000

label           loop
clear
variable        sname index hydrogen
units           electron
newton          on
boundary        p p p

atom_style      electron

read_data       data/T${my_temp}.lammps
read_data       data/T2000.lammps
  orthogonal box = (0 0 0) to (20.1478 20.1478 20.1478)
  2 by 2 by 4 MPI processor grid
  reading atoms ...
  864 atoms
  reading velocities ...
  864 velocities
pair_style      eff/cut 20.15
pair_coeff      * *
timestep        0.002
neigh_modify    one 20000 page 200000

#comm_modify	  vel yes
group           at type 1
432 atoms in group at
group	        el type 2
432 atoms in group el
reset_timestep  ${old_step}
reset_timestep  0
thermo          ${dumpstep}
thermo          1000

# dump            1 all cfg 10000 output/all/all.*.cfg mass type xs ys zs
# dump_modify     1 element C H

# dump            4  el cfg 10000 output/all/el.*.cfg mass type xs ys zs
# dump_modify     4 element C H

compute         1a all property/atom spin eradius ervel
compute     	peatom all pe/atom
compute		    keatom all ke/atom/eff
dump            2 all custom ${dumpstep} all/all.*.lammpstrj id type q c_1a[1] c_1a[2] x y z vx vy vz c_1a[3] c_peatom c_keatom
dump            2 all custom 1000 all/all.*.lammpstrj id type q c_1a[1] c_1a[2] x y z vx vy vz c_1a[3] c_peatom c_keatom

# compute       eff_energy components

compute         effTemp all temp/eff
compute         energies all pair eff/cut
variable        eke equal c_energies[1]
variable        epauli equal c_energies[2]
variable        ecoul equal c_energies[3]
variable        erres equal c_energies[4]

thermo_style    custom step pe temp press vol v_eke v_epauli v_ecoul v_erres lx
thermo_modify	temp effTemp

#dump            3 all xyz 1000 ${sname}.xyz

min_style       cg
min_modify      line quadratic
minimize        0 1.0e-2 1000 2000
